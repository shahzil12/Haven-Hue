{
    "version": 2,
    "framework": null,
    "functions": {
        "api/**/*.php": {
            "runtime": "vercel-php@0.7.2"
        }
    },
    "routes": [
        {
            "src": "/(css|js|images|storage|favicon.ico)(.*)",
            "dest": "/public/$1$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}